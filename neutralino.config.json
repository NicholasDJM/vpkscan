{
  "applicationId": "nicholas.miller.vpk.scanner",
  "version": "0.2.2",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/html/main.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "serverHeaders": {
    "Cache-Control": "max-age=31536000"
  },
  "nativeAllowList": [
    "app.exit",
    "app.restartProcess",
    "os.execCommand",
    "os.showFolderDialog",
    "os.open",
    "debug.log",
    "filesystem.readDirectory",
    "storage.getData",
    "storage.setData",
    "updater.*"
  ],
  "globalVariables": {
    "UPDATE_URL": "https://raw.githubusercontent.com/NicholasDJM/vpkscan/main/manifest.json"
  },
  "modes": {
    "window": {
      "title": "VPK Conflict Scanner",
      "width": 1200,
      "height": 900,
      "minWidth": 800,
      "minHeight": 500,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/favicon.ico",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {},
      "nativeBlockList": []
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": []
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": []
    }
  },
  "cli": {
    "binaryName": "vpkscan",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}